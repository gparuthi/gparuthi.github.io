alias gpf="git push --force-with-lease"
alias gs="git status"

# backup branch
gb(){
  # set -x
  dest=~/gitbkp 
  branch=$(git rev-parse --abbrev-ref HEAD)
  repo=$(git config --get remote.origin.url)
  mkdir -p $dest
  git clone -b $branch --single-branch $repo $dest/$branch--$(date "+%Y%m%d_%H%M")
}
